extends ../layout

block styles
    link(rel='stylesheet' href='/stylesheets/css/profile.css')

block title
    title users

block content
    include ../navbar
    .container
        .row
            table
                thead
                    tr
                        th Username
                        th First Name
                        th last Name
                tbody
                    each user in collaboratorAdminsList
                        tr
                            td #{user.username}
                            td #{user.fname}
                            td #{user.lname}
        .row
            table
                thead
                    tr
                        th Username
                        th First Name
                        th last Name
                tbody
